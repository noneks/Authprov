<% if user_signed_in? %>
  <%= form_for @picture, :html => { :multipart => true }   do |f| %>
  <div class="field">
    <%= f.label :picture %>
    <%= f.file_field :pic%></br>
    <%= f.submit %>
  </div>
  <% end %>
  <%= button_to 'Выйти', destroy_user_session_path, method: 'delete' %>
<% else %>
  <%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %></br>
  <%= link_to "Sign in with Vkontakte", user_omniauth_authorize_path(:vkontakte) %>
<% end %>